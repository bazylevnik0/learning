<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object id="window" class="GtkWindow">
    <property name="title">Isometric Rotation</property>
    <child>
        <object id="grid_global" class="GtkGrid">
            <child>
                <object class="GtkFrame" id="GtkFrameDraw">
                    <layout>
                        <property name="column">0</property>
                        <property name="row">0</property>
                    </layout>
                    <child type="label">
                        <object class="GtkLabel" id="frame_label_draw"/>
                    </child>
                </object>
            </child>
            <child>
                <object id="grid_control" class="GtkGrid">
                    <layout>
                        <property name="column">1</property>
                        <property name="row">0</property>
                    </layout>
                    <child>
                        <object id="button_icos" class="GtkButton">
                            <property name="label">icos</property>
                                <layout>
                                    <property name="column">0</property>
                                    <property name="row">0</property>
                                </layout>
                        </object>
                    </child>
                    <child>
                        <object id="button_para" class="GtkButton">
                            <property name="label">para</property>
                            <layout>
                                <property name="column">1</property>
                                <property name="row">0</property>
                            </layout>
                        </object>
                    </child>
                    <child>
                        <object class="GtkScale" id="scale_size">
                            <layout>
                                <property name="column">0</property>
                                <property name="column-span">2</property>
                                <property name="row">1</property>
                            </layout>
                            <property name="orientation">horizontal</property>
                            <property name="adjustment">adjustment_size</property>
                        </object>
                    </child>
                    <child>
                        <object class="GtkScale" id="scale_horizontal">
                            <layout>
                                <property name="column">0</property>
                                <property name="column-span">2</property>
                                <property name="row">2</property>
                            </layout>
                            <property name="orientation">horizontal</property>
                            <property name="adjustment">adjustment_horizontal</property>
                        </object>
                    </child>
                    <child>
                        <object class="GtkScale" id="scale_vertical">
                            <layout>
                                <property name="column">0</property>
                                <property name="column-span">2</property>
                                <property name="row">3</property>
                            </layout>
                            <property name="orientation">horizontal</property>
                            <property name="adjustment">adjustment_vertical</property>
                        </object>
                    </child>
                    </object>
                </child>
        </object>
    </child>
  </object>
  <object class="GtkAdjustment" id="adjustment_size">
    <property name="lower">0</property>
    <property name="upper">2</property>
    <property name="value">1</property>
    <property name="step-increment">0.1</property>
  </object>
  <object class="GtkAdjustment" id="adjustment_horizontal">
    <property name="lower">-25</property>
    <property name="upper">25</property>
    <property name="value">0</property>
    <property name="step-increment">1</property>
  </object>
  <object class="GtkAdjustment" id="adjustment_vertical">
    <property name="lower">-25</property>
    <property name="upper">25</property>
    <property name="value">0</property>
    <property name="step-increment">1</property>
  </object>
</interface>


